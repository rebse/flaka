<project xmlns:c="antlib:it.haefelinger.flaka">
  <c:let>
    _var_ = 'list'
  </c:let>
  <c:echo>
    #{_var_}
  </c:echo>
  <c:list var=" #{ _var_} ">
    file('.')
    ;; each line is a EL expression
    3 * 5
    ''.tofile.list
  </c:list>

  <c:for var="item" in=" #{_var_} ">
    <c:echo>#{item}</c:echo>
  </c:for>

</project>